@model Salon.ViewModel.FournisseurViewModel

<form asp-action="CreateFournisseur" method="post">
    <div class="form-group">
        <label for="Nom">Nom</label>
        <input type="text" class="form-control" id="Nom" name="Nom" value="@Model.Nom" required />
    </div>
    <div class="form-group">
        <label for="Contact">Contact</label>
        <input type="text" class="form-control" id="Contact" name="Contact" value="@Model.Contact" required />
    </div>
    <div class="form-group">
        <label for="Email">Email</label>
        <input type="email" class="form-control" id="Email" name="Email" value="@Model.Email" required />
    </div>
    <div class="form-group">
        <label for="Adresse">Adresse</label>
        <input type="text" class="form-control" id="Adresse" name="Adresse" value="@Model.Adresse" required />
    </div>
    <div class="form-group">
        <label for="Ville">Ville</label>
        <input type="text" class="form-control" id="Ville" name="Ville" value="@Model.Ville" required />
    </div>
    <div class="form-group">
        <label for="Pays">Pays</label>
        <input type="text" class="form-control" id="Pays" name="Pays" value="@Model.Pays" required />
    </div>
    <button type="submit" class="btn btn-primary">Ajouter</button>
</form>
